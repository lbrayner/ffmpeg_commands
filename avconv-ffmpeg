[a][b][c][d][e] concat=n=5:v=0:a=1 [p2]; \
[0:0]atrim=2.7:3.3[p3]; \
[0:0]atrim=2.75:2.9[p4]; \
[p1][p2][p3][p4] concat=n=4:v=0:a=1 [out]" \
-map "[out]" poesiia.mp3

# ffmpeg -y -i "2018-02-13_12-55-02.mp4" \
# -filter_complex \
# "[0:0]trim=3:36[a]; \
# [0:0]trim=40:43[b]; \
# [0:0]trim=125:130[c]; \
# [0:0]trim=137:210[d]; \
# [0:0]trim=236:332[e]; \
# [0:0]trim=357:769[f]; \
# [a][b][c][d][e][f] concat=n=6:v=1:a=0 [out]" \
# -vcodec libx264 -an \
# -map "[out]" "2018-02-13_12-55-02_cut.mp4"

ffmpeg -y -i "2018-02-13_12-55-02.mp4" \
-filter_complex \
"[0:0]trim=3:36,setpts=PTS-STARTPTS[a]; \
[0:0]trim=40:43,setpts=PTS-STARTPTS[b]; \
[a][b] concat=n=2:v=1:a=0 [out]" \
-vcodec libx264 -an \
-map "[out]" "2018-02-13_12-55-02_cut.mkv"

ffmpeg -y -i "WhatsApp Ptt 2018-02-08 at 21.53.31.ogg" \
-filter_complex \
"[0:0]atrim=0.0:0.6[esse]; \
[0:0]atrim=0.6:0.95[audio]; \
[0:0]atrim=0.95:1.9[aqui]; \
[0:0]atrim=1.9:1.95[in]; \
[in] asplit=10 [a][b][c][d][e][f][g][h][i][j]; \
[a][b][c][d][e][f][g][h][i][j] concat=n=10:v=0:a=1 [ta]; \
[0:0]atrim=2.1:2.35[muito]; \
[0:0]atrim=2.35:2.6[quente]; \
[0:0]atrim=2.6:4.0[velho]; \
[esse][quente][aqui][ta][muito][audio][velho] concat=n=7:v=0:a=1 [out]" \
-vn -acodec libmp3lame -ab 128k \
-map "[out]" quente.mp3

ffmpeg -y -i "2018-04-10_23-45-50.mp4" \
-filter_complex \
"[0:a]atrim=3.2:4.3,asetpts=PTS-STARTPTS[a]; \
[0:a]atrim=3.3:3.6,asetpts=PTS-STARTPTS[b]; \
[a][b] concat=n=2:v=0:a=1 [out]" \
-vn -acodec libmp3lame -ab 128k \
-map "[out]" querendo.mp3

ffmpeg -y -i "WhatsApp Ptt 2018-01-15 at 21.26.22.ogg" \
-filter_complex \
"[0:0]atrim=0.0:3.1[p1]; \
[0:0]atrim=3.1:3.5[p2]; \
[0:0]atrim=4.1:5.2[p3]; \
[0:0]atrim=0.38:0.9[ba]; \
[ba] asplit=3 [b1][b2][b3]; \
[p1][b1][p2][b2][p3][b3] concat=n=6:v=0:a=1 [pre]; \
[pre] volume=+10dB [out]" \
-vn -acodec libmp3lame -ab 128k \
-map "[out]" banha.mp3

ffmpeg -i input.mp4 -filter_complex "[0:v]trim=start=0:duration=90[a];\
[0:v]trim=start=90:duration=30,setpts=PTS-STARTPTS[b];\
[b]hflip[c];\
[a][c]concat[d];\
[0:v]trim=start=120:duration=60,setpts=PTS-STARTPTS[e];\
[d][e]concat[out1]" -map [out1] flip.mp4


ffmpeg -i tadeve.mp3 -filter_complex "[0:0]atrim=0:1[a];[0:0]atrim=1:2[b]; [a][a] concat=n=2:v=0:a=1 [c];[c][b]concat=n=2:v=0:a=1[out]" -map "[c]" tadeve_.mp3
ffmpeg -y -i tadeve.mp3 -filter_complex "[0:0]atrim=start=0:duration=0.3[a]; [0:0]atrim=start=0:duration=0.3[b]; [a][b] concat=n=2:v=0:a=1 [c]" -map "[c]" tadeve_.mp3
ffmpeg -y -i tadeve.mp3 -filter_complex "[0:0]atrim=0.2:0.3[in]; [0:0]atrim=1:2[in2]; [in] asplit=5 [a][b][c][d][e]; [a][b][c][d][e][in2] concat=n=6:v=0:a=1 [d]" -map "[d]" tadeve_.mp3
ffmpeg -y -i tadeve.mp3 -filter_complex "[0:0]atrim=0.2:0.3[in]" -map "[in]" ta.mp3
ffmpeg -y -i tadeve.mp3  -ss 0.2 -t 0.1 ta.mp3
sox ta.mp3 ta_.mp3 repeat 5

ffmpeg -y -i tadeve.mp3 -filter_complex \
"[0:0]atrim=0.2:0.3[in]; \
[in] asplit=5 [a][b][c][d][e]; \
[a][b][c][d][e] concat=n=5:v=0:a=1 [p1]; \
[0:0]atrim=0.95:1.3[in]; \
[in] asplit=5 [a][b][c][d][e]; \
[a][b][c][d][e] concat=n=5:v=0:a=1 [p2]; \
[0:0]atrim=1.25:1.6[p3]; \
[p1][p2][p3] concat=n=3:v=0:a=1 [out]" \
-map "[out]" tadeve_.mp3

ffmpeg -y -i cara.mp3 -filter_complex \
"[0:0]atrim=0.19:0.3[in]; \
[in] asplit=5 [a][b][c][d][e]; \
[a][b][c][d][e] concat=n=5:v=0:a=1 [p1]; \
[0:0]atrim=0.2:5[p3]; \
[p1][p3] concat=n=2:v=0:a=1 [out]" \
-map "[out]" kara.mp3


ffmpeg -y -i hamburguer.mp3 -i tainha.m4a -filter_complex \
"[0:0]atrim=0:2[p1]; \
[0:0]atrim=13.6:14.5[p2]; \
[0:0]atrim=13.6:14.5[p3]; \
[1:0]atrim=3.8:4.5[p4]; \
[p1][p2][p3][p4] concat=n=4:v=0:a=1 [out]" \
-map "[out]" buger.mp3

ffmpeg -i in.mov -vf "transpose=1" out.mov

ffmpeg -y -i input.m4v -metadata:s:v rotate="90" -codec copy output.m4v

# rotate
ffmpeg -y -i input.m4v -metadata:s:v rotate="270" -codec copy output.m4v


# mp4
ffmpeg -y -i input -vcodec libx264 -acodec copy -ss 00:01:20 output.mp4

# https://superuser.com/a/835585
ffmpeg -i <input_file> -af "highpass=f=200, lowpass=f=3000" <output_file>

# simple edit
ffmpeg -y -i input.ts -vcodec copy -acodec copy -ss 00:00:27 -to 00:05:58 flamengo_river_2019.mp4

# cut and reencode video
ffmpeg -y -i input.ts -vcodec libx264 -acodec copy -ss 00:00:27 -to 00:05:58 flamengo_river_2019.mp4

# reencode
ffmpeg -y -i input.ts -vcodec libx264 -acodec copy flamengo_river_2019.mp4

# audio only
ffmpeg -y -i input.ts -vn -acodec copy flamengo_river_2019.mp4

# m3u8
ffmpeg -y -live_start_index 0 -i "http://example.com/master.m3u8" -c copy -bsf:a aac_adtstoasc output.mp4

ffmpeg -headers $'Accept: */*' \
    -y -live_start_index 0 -i "http://example.com/master.m3u8" \
    -c copy -bsf:a aac_adtstoasc output.mp4

# aspect ratio
ffmpeg -i input -c copy -aspect 4:3 output

# The chunk is actually MPEG-TS but first 127 bytes appears to be a small PNG
# file affixed at the front. Forcing input format allows correct decoding.
ffmpeg -y -f mpegts -i in.ts -c copy out.mp4

# concat
ffmpeg -safe 0 -f concat -i mylist.txt -c copy out.mp4

# mux
ffmpeg -i video -i audio -vcodec copy -acodec copy -map 0:0 -map 1:0 output

# delaying audio stream by 2s
ffmpeg -i video -itsoffset 2.0 -i audio -vcodec copy -acodec copy -map 0:0 -map 1:0 output

# wav
ffmpeg -y -i input.wav -vn -acodec libmp3lame -ab 128k -af "volume=+10dB" output.mp3

# high pitched voice
ffmpeg -i input -vcodec copy -af "asetrate=44100*1.5, aresample=44100, atempo=1/1.5" output

# m4a increase volume
ffmpeg -y -i poesia.mp3 -vn -acodec aac -ab 125k -af "volume=+10dB" poesia.m4a

ffmpeg -y -i input.mp4 -vcodec copy -acodec aac -ab 125k -af "volume=+10dB" output.mp4

